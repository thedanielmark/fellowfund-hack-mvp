#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/danielmark/Projects/hackathons/ethglobal-bangkok/fellowfund-hack-mvp/api/node_modules/.pnpm/acorn@2.7.0/node_modules/acorn/bin/node_modules:/Users/danielmark/Projects/hackathons/ethglobal-bangkok/fellowfund-hack-mvp/api/node_modules/.pnpm/acorn@2.7.0/node_modules/acorn/node_modules:/Users/danielmark/Projects/hackathons/ethglobal-bangkok/fellowfund-hack-mvp/api/node_modules/.pnpm/acorn@2.7.0/node_modules:/Users/danielmark/Projects/hackathons/ethglobal-bangkok/fellowfund-hack-mvp/api/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/danielmark/Projects/hackathons/ethglobal-bangkok/fellowfund-hack-mvp/api/node_modules/.pnpm/acorn@2.7.0/node_modules/acorn/bin/node_modules:/Users/danielmark/Projects/hackathons/ethglobal-bangkok/fellowfund-hack-mvp/api/node_modules/.pnpm/acorn@2.7.0/node_modules/acorn/node_modules:/Users/danielmark/Projects/hackathons/ethglobal-bangkok/fellowfund-hack-mvp/api/node_modules/.pnpm/acorn@2.7.0/node_modules:/Users/danielmark/Projects/hackathons/ethglobal-bangkok/fellowfund-hack-mvp/api/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/acorn" "$@"
else
  exec node  "$basedir/../../bin/acorn" "$@"
fi
